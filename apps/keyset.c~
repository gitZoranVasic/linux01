#include <unistd.h>
#define UTIL_IMPLEMENTATION
#include "utils.h"
#include <errno.h>


int main(char* args) {

	int argNum = get_argc(args);

	if(argNum != 2) {
		_exit(-1);
	}
	int key = keyset(get_argv(args, 1));
		
	if(key < 0) {
		if(errno == EINVAL) 
			printerr("Kljuc nije ispravan. Mora biti stepen dvojke!\n");
		else printerr("Nepoznata greska!\n");
	}
	
	_exit(-1);

}
